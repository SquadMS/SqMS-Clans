{
    "name": "squadms/clans",
    "description": "The SquadMS Clans Management module.",
    "keywords": ["laravel", "squad", "squadms", "clans"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "nesbot/carbon": "^2.0",
        "squadms/foundation": "dev-navigation"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\Clans\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquadMS\\Clans\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\Clans\\ClansServiceProvider",
                "SquadMS\\Clans\\Providers\\EloquentServiceProvider",
                "SquadMS\\Clans\\Providers\\FilamentServiceProvider",
                "SquadMS\\Clans\\Providers\\ModulesServiceProvider",
                "SquadMS\\Clans\\Providers\\PermissionsServiceProvider",
                "SquadMS\\Clans\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
